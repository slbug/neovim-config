{
  "mcpServers": {
    "time": {
      "args": [
        "mcp-server-time",
        "--local-timezone=Europe/Warsaw"
      ],
      "command": "uvx"
    },
    "package-version": {
      "args": [
        "-y",
        "mcp-package-version"
      ],
      "command": "npx"
    },
    "fetch": {
      "args": [
        "mcp-server-fetch",
        "--ignore-robots-txt"
      ],
      "command": "uvx"
    }
  },
  "nativeMCPServers": [

  ]
}
